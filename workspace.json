          "options": {
            "jestConfig": "apps/profile-service/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/express"
  },
  "defaultProject": "accounts-service"
}
