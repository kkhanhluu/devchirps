      }
    },
    "profile-service": {
      "root": "apps/profile-service",
      "sourceRoot": "apps/profile-service/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
